///https://leetcode.com/problems/jump-game/description/?envType=study-plan-v2&envId=top-interview-150


class Solution {
public:
    bool canJump(vector<int>& nums) {
        int len = nums.size()-1;
        int jumps=0;
        for(int i=0;i<=jumps;i++){       //max value joto ashbe tototokoi jawa posible . [3,2,1,0,4] eita example
            jumps = max(jumps,i+nums[i]);
            if(jumps>=len) return true;   // value equal or greater hoile hobe.Karon jumps er value joto ami chaile tar kom beshi korte pari ðŸ˜Ž
        }
        return false;
    }
};
